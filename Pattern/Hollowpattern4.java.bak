import java.util.Scanner;
class  Hollowpattern4
{
	public static void P20(int n){
		for(int i=1;i<=n;i++){
			
			for(int j=1;j<=n;j++){
				if(i==1||j==1||(i+j)==5){
					System.out.print("*");
				}else{
					System.out.print(" ");
				}
			}
			System.out.println();
			
		}
		
	}
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		P20(n);
	}
}
